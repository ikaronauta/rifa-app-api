CREATE DATABASE rifa_db;
CREATE USER rifa_admin WITH ENCRYPTED PASSWORD 'Adm1n@123*';
GRANT ALL PRIVILEGES ON DATABASE rifa_db TO rifa_admin;

sudo systemctl restart postgresql

psql -U rifa_admin -d rifa_db -W



